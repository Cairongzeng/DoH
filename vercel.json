{
  "version": 2,
  "rewrites": [
    {
      "source": "/dns-query/:path*",
      "destination": "/api/doh"
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=600, stale-while-revalidate=30"
        }
      ]
    },
    {
      "source": "/dns-query/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=600, stale-while-revalidate=600"
        }
      ]
    }
  ]
}

